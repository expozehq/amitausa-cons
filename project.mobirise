0.5\" condition=\"overlay && bg.type !== 'color'\">\n        <!-- End block parameters -->\n    </mbr-parameters>\n\n    <div class=\"mbr-overlay\" mbr-if=\"overlay && bg.type!== 'color'\" mbr-style=\"{'opacity': overlayOpacity, 'background-color': overlayColor}\">\n    </div>\n    <div class=\"container\">\n\n\n\n\n        <div class=\"row justify-content-center\">\n            <div class=\"card card1 p-3 col-12 col-md-7 col-lg-4\">\n                <div class=\"card-wrapper\">\n\n\n                    <span mbr-icon class=\"mbr-iconfont mobi-mbri-user mobi-mbri\" mbr-if=\"showIcon\"></span>\n                    <div class=\"card-box align-left\">\n                        <h4 class=\"card-title mbr-section-subtitle mbr-regular pb-3 mbr-black mbr-fonts-style\" mbr-theme-style=\"display-4\" mbr-if=\"showTitle\" data-app-selector=\".card-title\"></h4>\n                        <h5 class=\"card-text mbr-regular mbr-black mbr-fonts-style\" mbr-theme-style=\"display-7\" mbr-if=\"showText\" data-app-selector=\".card-text\">Др. Татьяна Яковлева,<br>L.Ac, RMT</h5>\n                    </div>\n\n                </div>\n            </div>\n\n\n\n            <div class=\"card card2 p-3 col-12 col-md-7 col-lg-4\" mbr-if=\"cardsAmount > 1\">\n\n\n                <div class=\"card-wrapper\">\n\n\n                    <span mbr-icon class=\"mbr-iconfont mobi-mbri-users mobi-mbri\" mbr-if=\"showIcon\"></span>\n                    <div class=\"card-box align-left\">\n                        <h4 class=\"card-title mbr-section-subtitle mbr-regular pb-3 mbr-black mbr-fonts-style\" mbr-theme-style=\"display-4\" mbr-if=\"showTitle\" data-app-selector=\".card-title\">WRITTE ON\n                        </h4>\n                        <h5 class=\"card-text mbr-regular mbr-black mbr-fonts-style\" mbr-theme-style=\"display-4\" mbr-if=\"showText\" data-app-selector=\".card-text\">© <a href=\"https://amitausa.com/\" target=\"_blank\" class=\"text-success\">AMITA HOLISTIC HEALING CENTER</a>, 2020\n</h5>\n                    </div>\n\n                </div>\n            </div>\n\n            <div class=\"card card3 p-3 col-12 col-md-7 col-lg-4\" mbr-if=\"cardsAmount > 2\">\n                <div class=\"card-wrapper\">\n\n\n                    <span mbr-icon class=\"mbr-iconfont mobi-mbri-sad-face mobi-mbri\" mbr-if=\"showIcon\"></span>\n                    <div class=\"card-box align-left\">\n                        <h4 class=\"card-title mbr-section-subtitle mbr-regular pb-3 mbr-black mbr-fonts-style\" mbr-theme-style=\"display-4\" mbr-if=\"showTitle\" data-app-selector=\".card-title\">FREE CALLS ON\n                        </h4>\n                        <h5 class=\"card-text mbr-regular mbr-black mbr-fonts-style\" mbr-theme-style=\"display-4\" mbr-if=\"showText\" data-app-selector=\".card-text\">MADE AND POWERED BY <a href=\"http://md.expo-products.com/\" target=\"_blank\">EXPO PRODUCTS s.r.o.\n</a></h5>\n                    </div>\n\n                </div>\n            </div>\n\n\n        </div>\n    </div>\n</section>",
